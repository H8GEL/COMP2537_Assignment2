<%- include("templates/header") %>
<div class="d-flex justify-content-center align-items-center">
<form action="/loggingin" method="post" class="text-center m-4">
    <div class="form-group m-4">
        <label class="m-1">Username</label>
        <input name="username" type="text" class="form-control" id="usernameExample" placeholder="username">
    </div>
    <div class="form-group m-4">
        <label class="m-1">Password</label>
        <input name="password" type="password" class="form-control" id="passwordExample" placeholder="password">
    </div>
    <button type="submit" class="btn btn-primary ms-3">Log In</button>

</form> 
</div>
<%- include("templates/footer") %>